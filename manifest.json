{
  "name": "MyTestChromeAddon",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chromeアドオンを自作してみるテスト。",
  "permissions": [ "*://*/*"],
  "content_scripts": [ {
    "all_frames": true,
    "matches": ["*://*/*"],
    "run_at": "document_start",
    "css": ["uplus-misc.css"],
    "js": ["jquery.min.js", "foo.js"]
  }]
}
